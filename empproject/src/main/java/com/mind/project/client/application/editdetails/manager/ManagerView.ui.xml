<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel ui:field="contentPanel">
		<table align="center">
			<hr />

			<tr>
				<td>
				</td>
				<td>
					<h3> Assign Employee to the Manager</h3>
				</td>
			</tr>



			<tr>
				<td>
					<h3>Change Reportee for</h3>
				</td>
				<td>
					<g:ListBox ui:field="managers" multipleSelect="false"
						title="Allowed Managers" width="100px">
					</g:ListBox>
				</td>
			</tr>



			<tr>
				<td>
					<g:Label>AvailableEmployees</g:Label>
				</td>
				<td>
				</td>
				<td>
					<g:Label>Assigned Employees</g:Label>
				</td>
			</tr>



			<tr>
				<td>
					<g:ListBox ui:field="availableEmployees" multipleSelect="true"
						title="Employees" width="150px">
					</g:ListBox>
				</td>
				<td>
					<g:Button ui:field="addButn"> ADD </g:Button>
					<g:Button ui:field="delButn"> DELETE </g:Button>
				</td>
				<td>
					<g:ListBox ui:field="assignedEmployees" multipleSelect="true"
						title="Reportees" width="150px">
					</g:ListBox>
				</td>
			</tr>
		</table>



		<table align="center">
			<tr>
				<td>
					<g:Button ui:field="saveButton">SAVE</g:Button>
				</td>
			</tr>
		</table>

	</g:HTMLPanel>
</ui:UiBinder>
